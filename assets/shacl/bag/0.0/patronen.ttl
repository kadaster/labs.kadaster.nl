  shp:huisnummertoevoeging
    a sh:PropertyShape;#
    sh:datatype xsd:string;#
    sh:description "Maximaal vier alfanumerieke tekens bestaande uit een combinatie van hoofdletters (A – Z), kleine letters (a – z) en/of cijfers (0 – 9)."@nl;
    sh:maxCount 1;
    sh:name "heeft huisnummertoevoeging"@nl;#
    sh:nodeKind sh:Literal;#
    sh:path bag:huisnummertoevoeging;
    sh:pattern "[0-9A-Za-z]{1,4}".


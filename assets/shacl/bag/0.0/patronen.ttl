  shp:NummeraanduidingRegistratie_documentdatum
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#739-documentdatum"^^xsd:anyURI;

@prefix gebruiksdoel: <https://bag2.basisregistraties.overheid.nl/bag/id/gebruiksdoel/> .
@prefix shp: <https://bag2.basisregistraties.overheid.nl/bag/shp/> . 
@prefix status: <https://bag2.basisregistraties.overheid.nl/bag/id/status/>. 

  shp:Identificatie
    a sh:ShapeNode;#
    sh:closed true;#
    sh:description "De BAG volgt de NEN 3610 standaard voor het vastleggen van de identificatie van ieder geregistreerd object."@nl;
    sh:ignoredProperties ( rdf:type );#
    sh:name "Identificatie"@nl;
    sh:property
      shp:lokaalId,
      shp:namespace;
    sh:targetClass nen3610:Identificatie.

  shp:lokaalId
    a sh:PropertyShape;#
    sh:datatype xsd:string;
    sh:description "De identificatiecode voor een BAG object.  Deze code is uniek binnen de BAG namespace (vandaar de term: lokaal)."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft lokale identificatie"@nl;
    sh:nodeKind sh:Literal;#
    sh:path nen3610:lokaalID.

  shp:namespace
    a sh:PropertyShape;#
    sh:datatype xsd:string;
    sh:description "De identificatiecodes voor BAG objecten zijn unieke binnen de context van deze naamgevingsruimte."@nl;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft naamgevingsruimte"@nl;
    sh:nodeKind sh:Literal;#
    sh:path nen3610:namespace;
    sh:pattern "^NL.IMBAG.".

  shp:value
    a sh:PropertyShape;#
    sh:datatype xsd:string;
    sh:description "De rdf:value mag gebruikt worden om het NEN3610ID als één string te beschrijven. Daarbij dient de waarde opbouwd te zijn volgens het sjabloon {namespace}{lokaalID}{versie}"@nl;
    sh:maxCount 1;
    sh:name "de gecombineerde identificatie"@nl;
    sh:nodeKind sh:Literal;#
    sh:property
      shp:lokaalID,
      shp:namespace;
    sh:path rdf:value.


  shp:Ligplaats
    a sh:ShapeNode;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:name "Ligplaats"@nl;#
    sh:property shp:Ligplaats_identificatie;
    sh:targetClass bag:Ligplaats.

  shp:Ligplaats_identificatie
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#761-identificatie"^^xsd:anyURI;
    sh:class nen3610:Identificatie;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft identificatie"@nl;
    sh:nodeKind sh:IRI;#
    sh:path nen3610:identificatie;
    skos:definition "De unieke aanduiding van een ligplaats."@nl;
    skos:scopeNote "Elke Ligplaats waarvan gegevens zijn opgenomen in de basisregistratie adressen en gebouwen wordt uniek aangeduid door middel van een identificatiecode."@nl.

  shp:LigplaatsRegistratie
    a sh:ShapeNode;#
    sh:closed true;#
    sh:ignoredProperties ( rdf:type );#
    sh:name "Ligplaats registratie"@nl;#
    sh:property
      shp:formeleHistorieBegin,#
      shp:formeleHistorieBeginLV,#
      shp:formeleHistorieEind,#
      shp:formeleHistorieEindLV,#
      shp:materiëleHistorieBegin,#
      shp:materiëleHistorieEind,#
      shp:primaryTopic,#
      shp:voorkomenIdentificatie,#
      shp:LigplaatsRegistratie_documentdatum,
      shp:LigplaatsRegistratie_documentnummer,
      shp:LigplaatsRegistratie_formeleLevensduur,
      shp:LigplaatsRegistratie_geconstrateerd,
      shp:LigplaatsRegistratie_geometrie,
      shp:LigplaatsRegistratie_materiëleLevensduur,
      shp:LigplaatsRegistratie_status,
      shp:hoofdadres,
      shp:nevenadres;
    sh:targetClass bag:LigplaatsRegistratie.

  shp:LigplaatsRegistratie_documentdatum
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#765-documentdatum"^^xsd:anyURI;
    sh:datatype xsd:date;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft documentdatum"@nl;
    sh:nodeKind sh:Literal;#
    sh:path dct:created;
    skos:definition "De datum waarop het brondocument is vastgesteld op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een ligplaats heeft plaatsgevonden."@nl;
    skos:scopeNote "In het geval het brondocument een besluit van een bestuursorgaan betreft, dan is de datum van het brondocument de datum waarop het besluit is genomen."@nl.

  shp:LigplaatsRegistratie_documentnummer
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#766-documentnummer"^^xsd:anyURI;
    sh:datatype xsd:string;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft documentnummer"@nl;
    sh:nodeKind sh:Literal;#
    sh:path dct:identifier;
    sh:pattern "[0-9]{1,40}";
    skos:definition "De unieke aanduiding van het brondocument op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een ligplaats heeft plaatsgevonden binnen een gemeente."@nl.

  shp:LigplaatsRegistratie_formeleLevensduur
    a sh:PropertyShape;#
    sh:class bag:LigplaatsRegistratie;
    sh:description "Wordt gebruikt om de formele levensduur mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft vorige geregistreerde versie"@nl;
    sh:nodeKind sh:IRI;#
    sh:path nen3610:vorigeGeregistreerdeVersie.

  shp:LigplaatsRegistratie_geconstrateerd
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#764-geconstateerd"^^xsd:anyURI;
    sh:datatype xsd:boolean;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "is geconstateerd"@nl;#
    sh:nodeKind sh:Literal;#
    sh:path bag:geconstateerd;
    skos:definition "Een aanduiding waarmee kan worden aangegeven dat een ligplaats in de registratie is opgenomen als gevolg van een feitelijke constatering, zonder dat er op het moment van opname sprake was van een regulier brondocument voor deze opname."@nl;
    skos:scopeNote "Het feitelijk bestaan van een ligplaats dient daadwerkelijk te worden geconstateerd. Het gaat dan bijvoorbeeld om het feitelijk permanent gebruik van een plaats in het water dat binnen andere processen aan het licht is gekomen."@nl.

  shp:LigplaatsRegistratie_geometrie
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#763-geometrie"^^xsd:anyURI;
    sh:datatype geo:wktLiteral;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft geometrie"@nl;#
    sh:nodeKind sh:Literal;#
    sh:path bag:geometrie;
    skos:definition "De tweedimensionale geometrische representatie van de omtrekken van een ligplaats."@nl.

  shp:LigplaatsRegistratie_materiëleLevensduur
    a sh:PropertyShape;#
    sh:class bag:LigplaatsRegistratie;
    sh:description "Wordt gebruikt om de materiële levensduur mee aan de duiden."@nl;
    sh:maxCount 1;
    sh:name "heeft vorige geldige versie"@nl;
    sh:nodeKind sh:IRI;#
    sh:path nen3610:vorigeGeldigeVersie.

  shp:LigplaatsRegistratie_status
    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Gebouwen Registratie versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#762-status"^^xsd:anyURI;
    sh:class bag:StatusPlaats;
    sh:in#
      ( status:plaatsAangewezen#
        status:plaatsIngetrokken );#
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft status"@nl;#
    sh:nodeKind sh:IRI;#
    sh:path bag:status;
    skos:definition "De fase van de levenscyclus van een ligplaats waarin de betreffende ligplaats zich bevindt."@nl;
    skos:scopeNote "De BAG bevat gegevens over zowel daadwerkelijk aanwezige ligplaatsen als ingetrokken ligplaatsen. Om het onderscheid tussen de stadia in de levenscyclus van de ligplaats te kunnen maken, wordt gebruik gemaakt van dit statusgegeven."@nl.

  shp:hoofdadres
    a sh:PropertyShape;#
    sh:class bag:Nummeraanduiding;#
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeft hoofdadres"@nl;#
    sh:nodeKind sh:IRI;#
    sh:path bag:hoofdadres.

  shp:nevenadres
    a sh:PropertyShape;#

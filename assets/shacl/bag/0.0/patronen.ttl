    a sh:PropertyShape;#
    dct:source "Grondslagen Basis Registratie Adressen versie 4.0"@nl;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#737-status"^^xsd:anyURI;
